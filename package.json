{
  "name": "@bosh-code/eslint-plugin",
  "version": "0.0.0",
  "description": "Ryan Bosher's ESLint plugin.",
  "author": "Ryan Bosher <ryan@bosher.co.nz>",
  "contributors": [
    {
      "name": "Ryan Bosher",
      "email": "ryan@bosher.co.nz",
      "url": "https://bosher.co.nz"
    }
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bosh-code/eslint-plugin-bosh-code.git"
  },
  "scripts": {
    "build": "tsdown",
    "safe-publish-latest": "safe-publish-latest",
    "prepublishOnly": "run-s safe-publish-latest build lint",
    "lint": "eslint ."
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.16.1",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "eslint": "^9.35.0"
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@typescript-eslint/utils": "^8.43.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^57.0.7",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.22.0",
    "eslint-plugin-no-unary-plus": "^0.3.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "safe-publish-latest": "^2.0.0",
    "typescript-eslint": "^8.43.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "eslint": "^9.35.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.1",
    "typescript": "^5.9.2"
  }
}
